{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
      "include": ["./lib/platform.js"]
    },
    "plugins": [ "plugins/markdown" ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": false,
        "private": true,
        "lenient": false
    }
}